<view class="page"><view class="tabBar"><block wx:for="{{TabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{[activeIndex==index?'tab-active':'']}}" bindtap="__e">{{''+item+''}}</text></block></view><view class="content"><block wx:for="{{videoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['enterVideo',['$event']]]]]}}" class="video" bindtap="__e"><view class="picture"><image src="{{item.img}}" mode></image><image class="play-icon" src="../../static/play.png" mode></image></view><view class="message"><text class="title">{{item.title}}</text><view class="info"><view class="date"><image src="../../static/icon_time.png" mode="widthFix"></image><text>{{item.date}}</text></view><view class="visit"><image src="../../static/icon_browse.png" mode="widthFix"></image><text>{{item.visiter}}</text></view></view></view></view></block></view></view>