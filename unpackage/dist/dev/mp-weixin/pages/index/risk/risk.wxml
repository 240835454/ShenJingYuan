<view class="page"><view class="tabBar"><block wx:for="{{TabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" class="{{[activeIndex==index?'tab-active':'']}}" bindtap="__e">{{''+item+''}}</text></block></view><view class="content"><view class="box"><view class="picture"><image src="../../../static/fengxian.png" mode="widthFix"></image><text class="test-result">{{''+rate+''}}</text><text class="result">{{"测试结果为："+result}}</text><text class="result">{{"最佳风险值："+best}}</text><text data-event-opts="{{[['tap',[['enterReport',['$event']]]]]}}" class="report" bindtap="__e">查看评估风险报告</text></view></view><block wx:for="{{infoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="info-item"><text class="name">{{''+item.name+''}}</text><text class="value">{{''+item.value+''}}</text></view></block><view data-event-opts="{{[['tap',[['historyRecord',['$event']]]]]}}" class="info-item" bindtap="__e"><text class="name">查看历史评估记录</text><text class="value"></text></view><view class="footer"><text data-event-opts="{{[['tap',[['beginEvaluate',['$event']]]]]}}" class="button" bindtap="__e">开始评估</text></view></view></view>